        <property name="printfile" type="string" />
      </object>
      <object name="Run_responses_default_def" />
    </jsonTypes>
  </grammars>
  <resources siena:authenticationProviderHref="#PowerAppAuth" base="https://canada-001.azure-apim.net/apim/logicflows">
    <resource path="/{connectionId}/triggers/manual/run">
      <param style="template" name="connectionId" type="xs:string" required="true" />
      <method siena:requiresAuthentication="true" name="POST" id="Run">
        <doc title="Runs a logic flow trigger." />
        <request>
          <param style="query" name="api-version" type="xs:string" required="true" fixed="2015-02-01-preview">
            <doc title="The api version" />
          </param>
          <representation mediaType="application/json">
            <param style="plain" name="text" path="/text" type="xs:string" required="true">
              <doc title="Category" />
            </param>
            <param style="plain" name="text_1" path="/text_1" type="xs:string" required="true">
              <doc title="Log Name" />
            </param>
            <param style="plain" name="text_2" path="/text_2" type="xs:string" required="true">
              <doc title="Schedule Date" />
            </param>
            <param style="plain" name="text_3" path="/text_3" type="xs:string" required="true">
              <doc title="Maintenance Details 1" />
            </param>
            <param style="plain" name="text_4" path="/text_4" type="xs:string">
              <doc title="Maintenance Details 2" />
            </param>
            <param style="plain" name="text_5" path="/text_5" type="xs:string">
              <doc title="Maintenance Details 3" />
            </param>
            <param style="plain" name="text_6" path="/text_6" type="xs:string">
              <doc title="Maintenance Details 4" />
            </param>
            <param style="plain" name="text_7" path="/text_7" type="xs:string">
              <doc title="Maintenance Details 5" />
            </param>
            <param style="plain" name="text_8" path="/text_8" type="xs:string">
              <doc title="Maintenance Details 6" />
            </param>
            <param style="plain" name="text_9" path="/text_9" type="xs:string">
              <doc title="Maintenance Details 7" />
            </param>
            <param style="plain" name="text_10" path="/text_10" type="xs:string">
              <doc title="Maintenance Details 8" />
            </param>
            <param style="plain" name="text_11" path="/text_11" type="xs:string">
              <doc title="Maintenance Details 9" />
            </param>
            <param style="plain" name="text_12" path="/text_12" type="xs:string">
              <doc title="Maintenance Details 10" />
            </param>
            <param style="plain" name="text_13" path="/text_13" type="xs:string" required="true">
              <doc title="Store Details" />
            </param>
            <param style="plain" name="text_14" path="/text_14" type="xs:string" required="true">
              <doc title="Language" />
            </param>
          </representation>
        </request>
        <response>
          <representation mediaType="application/json" element="service:ResponseActionOutput" />
        </response>
      </method>
    </resource>
  </resources>
  <authenticationProviders xmlns="http://schemas.microsoft.com/MicrosoftProjectSiena/WADL/2014/11">
    <connectionProvider id="PowerAppAuth" siena:connectionProviderId="logicflows" />
  </authenticationProviders>
  <template xmlns="http://schemas.microsoft.com/MicrosoftProjectSiena/WADL/2014/11">
